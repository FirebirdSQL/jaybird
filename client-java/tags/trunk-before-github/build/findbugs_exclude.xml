<FindBugsFilter>
	<Match>
	  <!-- the Match filter is equivalent to a logical 'And' -->

	  <Class name="~.*\.Test.*" />
	  <!-- test classes are prefixed by 'Test' -->

	  <Not>
		  <Bug code="IJU" /> <!-- 'IJU' is the code for bugs related to JUnit test code -->
	  </Not>
	</Match>
</FindBugsFilter>