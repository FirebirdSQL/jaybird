<?xml version="1.0"?>
<project name="util">
    <!-- =================================================================== -->
    <!-- Fix cr-lf                                                           -->
    <!-- =================================================================== -->
    <target name="pretty" depends="init">
        <fixcrlf srcDir="${source.java}"
                 includes="**/*.java,**/*.xml,**/*.sh"
                 cr="remove"
                 tab="remove"
                 tablength="4"
                 eof="remove"/>
    </target>

    <!-- =================================================================== -->
    <!-- Cleans up generated stuff                                           -->
    <!-- =================================================================== -->
    <target name="clean" depends="init">
        <delete dir="${module.output}"/>
    </target>

</project>